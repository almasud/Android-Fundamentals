<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".content_provider.ContentProviderActivity">

    <Button
        android:id="@+id/buttonContactProviderActivity"
        android:layout_width="193dp"
        android:layout_height="67dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="110dp"
        android:text="Contact Provider Activity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textAllCaps="false"
        android:onClick="contactProviderActivity"/>

    <Button
        android:id="@+id/buttonEventProvider"
        android:layout_width="195dp"
        android:layout_height="61dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="108dp"
        android:text="Event Provider Activity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonContactProviderActivity"
        android:textAllCaps="false"
        android:onClick="eventProviderActivity"/>
</androidx.constraintlayout.widget.ConstraintLayout>